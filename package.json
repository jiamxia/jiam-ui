{
  "name": "jiam-ui",
  "version": "0.0.4",
  "description": "React components of my",
  "main": "index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "jiam927@163.com",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "react",
    "component",
    "react-component",
    "ui",
    "frontend"
  ],
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist && rimraf lib",
    "build:tsc": "tsc -p tsconfig.build.json --target ES5 --outDir lib",
    "build:es": "cpr src/styles lib/styles -o && cpr src/assets lib/assets -o",
    "build": "npm run clean && npm run build:prod && npm run build:dev && npm run build:tsc && npm run build:es",
    "release": "sh ./scripts/release.sh",
    "publish": "sh ./scripts/publish.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "changeissueurl": "replace 'https://github.com/myproject/issues/' 'https://redmine.example.com/' CHANGELOG.md",
    "prepare": "husky install",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --quiet  --ignore-path .gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:style": "stylelint src/**/*.{css,less,scss} --fix ",
    "precommit": "lint-staged",
    "commit": "cz",
    "dumi": "cross-env APP_ROOT=dumi dumi dev",
    "dumi-build": "cross-env APP_ROOT=dumi dumi build"
  },
  "bugs": {
    "url": "https://github.com/jiamxia/jiam-ui/issues",
    "email": "jiam927@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiamxia/jiam-ui.git"
  },
  "files": [
    "dist/*",
    "lib",
    "index.js",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "get-intrinsic": "1.0.1",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@emotion/core": "^11.0.0",
    "@loadable/webpack-plugin": "^5.14.2",
    "@types/classnames": "^2.2.11",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "acorn": "^8.4.1",
    "autoprefixer": "8.0.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cpr": "^3.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.0",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "dumi": "^1.1.25",
    "eslint": "7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "gatsby-plugin-sass": "3.2.0",
    "husky": "^7.0.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.2",
    "make-dir-cli": "^2.0.0",
    "mini-css-extract-plugin": "^1.4.0",
    "nrm": "^1.2.1",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "semver": "^7.3.5",
    "shelljs": "^0.8.4",
    "speed-measure-webpack-plugin": "^1.5.0",
    "standard-version": "^9.3.1",
    "terser-webpack-plugin": "^5.1.4",
    "ts-loader": "^8.1.0"
  },
  "lint-staged": {
    "src/**/*.js": "prettier --write --ignore-unknown"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}